{
  "$schema": "https://aka.ms/azure/static-web-apps-cli/schema",
  "configurations": {
    "expense-tracker": {
      "appLocation": "src\\ExpenseTracker.Web.Client",
      "apiLocation": "src\\ExpenseTracker.Web.Api",
