{
  "routes": [
    {
      "route": "/api/*",
      "allowedRoles": [ "authenticated" ]
    },
    {
      "route": "/logout",
      "redirect": "/.auth/logout"
    },
    {
      "route": "/login/twitter",
      "redirect": "/.auth/login/twitter"
    },
    {
      "route": "/login/github",
      "redirect": "/.auth/login/github"
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html"
  }
}